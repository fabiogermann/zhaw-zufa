	scale 50

\end_inset


\end_layout

\begin_layout Verse
\begin_inset Formula $r\cdot e^{i\gamma}=a+i\cdot b=\sqrt{a^{2}+b^{2}}\cdot e^{i\cdot arctan(\frac{b}{a})}$
\end_inset


\end_layout

