\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\frac{3x-5}{x^{2}+2x-8}=\frac{A}{(x-2)}+\frac{B}{(x+4)}$
\end_inset

 
\begin_inset Formula $\mid\cdot(x-2)\cdot(x+4)$
\end_inset


\end_layout

