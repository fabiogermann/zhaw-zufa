\begin_inset Formula $3x-5=A(x+4)+B(x-2)$
\end_inset

 
\begin_inset Formula $\mid$
\end_inset

x einsetzen und A, B ausrechnen (z.B.
 x=-4, x=2)
\end_layout

\begin_layout Itemize
\begin_inset Formula $A=\frac{1}{6};B=\frac{17}{6}$
\end_inset


\end_layout

\begin_layout Itemize

