\begin_layout Standard
Trick: Zähler eines Bruches so korrigieren, das es der Ableitung der Funktion
 entspricht.
 Anschliessend kann 
\begin_inset Formula $u^{\prime}\cdot dx$
\end_inset

 durch 
\begin_inset Formula $du$
\end_inset

 ersetzt werden.
\end_layout

\begin_layout Itemize
\begin_inset Formula $\int\frac{1}{(4x+5)^{3}}\cdot dx=\frac{1}{4}\cdot\int\frac{4}{(4x+5)^{3}}\cdot dx=\frac{1}{4}\cdot\int\frac{u^{\prime}}{u^{3}}\cdot dx=\frac{1}{4}\cdot\int\frac{du}{u^{3}}=\frac{1}{4}\cdot\frac{u^{-2}}{-2}+c=-\frac{1}{8u^{2}}+c=-\frac{1}{8}\cdot\frac{1}{(4x+e)^{2}}+c$
\end_inset

 wobei 
\begin_inset Formula $dx=\frac{du}{u}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\int\frac{x}{\sqrt{a^{2}-x^{2}}}\cdot dx=\int\frac{x}{\sqrt{u}}\cdot dx=\int\frac{x}{\sqrt{u}}\cdot\frac{du}{u^{\prime}}=\int\frac{x\cdot du}{-2x\cdot\sqrt{u}}=-\int\frac{du}{2\cdot\sqrt{u}}=-\frac{1}{2}\cdot u^{\frac{1}{2}}\cdot2+c=-\sqrt{u}+c=-\sqrt{a^{2}-x^{2}}+c$
\end_inset


\end_layout

