\begin_layout Part*
Schwingungen
\end_layout

\begin_layout Standard
Allgemein
\end_layout

\begin_layout Itemize
Je grösser die Federkonstante k, umso kleiner die Auslenkung bei gleicher
 Kraft (resp.
 umso steifer die Feder)
\end_layout

\begin_layout Itemize
Je steifer die Feder, desto grösser/höher ist die Schwingfrequenz 
\begin_inset Formula $\omega$
\end_inset

.
\end_layout

\begin_layout Itemize
Je grösser die Masse m, umso kleiner/tiefer ist die Schwingfrequenz 
\begin_inset Formula $\omega$
\end_inset

.
\end_layout

\begin_layout Standard
Formelherleitung:
\end_layout

\begin_layout Itemize
\begin_inset Formula $m\ddot{x}=-k(x-x_{Ruhe})-\gamma\cdot\dot{x}\:\Longleftrightarrow\ddot{x}+\frac{\gamma}{m}\cdot\dot{x}+\frac{k}{m}(x-x_{Ruhe})=0$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $m\ddot{x}=\sum F_{i}=F_{G}+F_{k}+F_{r}\:\Longrightarrow\ddot{x}=acc=\frac{1}{m}\cdot(F_{G}+F_{k}+F_{r})$
\end_inset


\end_layout

\begin_layout Section*
Ungedämpfte Schwingung
\end_layout

\begin_layout Verse
\begin_inset Formula $\omega=\sqrt{\frac{k}{m}}$
\end_inset

, k = Federkonstante, m = Masse
\end_layout

\begin_layout Verse
\begin_inset Formula $x(t)=A\cdot cos(\omega t+B)$
\end_inset

, A=Amplitude, B=Phase 
\end_layout

\begin_layout Section*
Schwach gedämpfte Schwingung
\end_layout

\begin_layout Verse
\begin_inset Formula $\omega=\sqrt{\frac{k}{m}-\delta^{2}}$
\end_inset

, 
\begin_inset Formula $\delta=\frac{\gamma}{2m}$
\end_inset

 k = Federkonstante, m = Masse, 
\begin_inset Formula $\delta$
\end_inset

=Dämpfung
\end_layout

\begin_layout Verse
\begin_inset Formula $\frac{k}{m}-\delta^{2}>0$
\end_inset


\end_layout

\begin_layout Verse
\begin_inset Formula $x(t)=Ae^{-\delta t}\cdot cos(\omega t+B)$
\end_inset

, A=Amplitude, B=Phase 
\end_layout

\begin_layout Section*
Starke (überkritisch) gedämpfte Schwingung
\end_layout

\begin_layout Verse
\begin_inset Formula $\omega=\sqrt{\frac{k}{m}-\delta^{2}}$
\end_inset

, 
\begin_inset Formula $\delta=\frac{\gamma}{2m}$
\end_inset

 k = Federkonstante, m = Masse, 
\begin_inset Formula $\delta$
\end_inset

=Dämpfung
\end_layout

\begin_layout Verse
\begin_inset Formula $\frac{k}{m}-\delta^{2}<0$
\end_inset


\end_layout

\begin_layout Verse
\begin_inset Formula $x(t)=Ae^{-(\delta+\sqrt{(\frac{\gamma}{2m})^{2}-\frac{k}{m}})}+Be^{-(\delta-\sqrt{(\frac{\gamma}{2m})^{2}-\frac{k}{m}})}$
\end_inset


\end_layout

