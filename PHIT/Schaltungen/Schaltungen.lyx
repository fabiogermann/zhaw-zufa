\begin_layout Verse
\begin_inset Formula $B=\mu_{0}\cdot\frac{N}{L}\cdot I=\mu_{r}\cdot\mu_{0}\cdot\frac{N}{L}\cdot I=\mu_{r}\cdot\mu_{0}\cdot\frac{N\cdot U}{L\cdot R}=\frac{\phi}{A}$
\end_inset


\end_layout

\begin_layout Verse
\begin_inset Formula $\mu_{0}=4\pi\cdot10^{-7}Vs/Am$
\end_inset


\end_layout

\begin_layout Section*
