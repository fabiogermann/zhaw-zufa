\begin_layout Subsection
Empirische Varianz
\end_layout

\begin_layout Verse
\begin_inset Formula $s^{2}=\frac{1}{n}\overset{n}{\underset{i=1}{\sum}}(x_{i}-\bar{x})^{2}f(x_{i})$
\end_inset


\end_layout

\begin_layout Subsection
Empirische Standardabweichung
\end_layout

\begin_layout Verse
\begin_inset Formula $s=\sqrt{s^{2}}$
\end_inset


\end_layout

\begin_layout Standard
Dies hat folgende bedeutung:
\end_layout

\begin_layout Itemize
etwa 68.3% aller Messwerte liegen im Intervall 
\begin_inset Formula $[\bar{x}-s,\bar{x}+s]$
\end_inset


\end_layout

\begin_layout Itemize
etwa 95.5% aller Messwerte liegen im Intervall 
\begin_inset Formula $[\bar{x}-2s,\bar{x}+2s]$
\end_inset


\end_layout

\begin_layout Itemize
etwa 99.7% aller Messwerte liegen im Intervall 
\begin_inset Formula $[\bar{x}-3s,\bar{x}+3s]$
\end_inset


\end_layout

