\begin_inset Formula $f(a_{1}),...,f(a_{k})$
\end_inset

 gilt:
\end_layout

\begin_layout Verse
\begin_inset Formula $\bar{x}=\overset{k}{\underset{i=1}{\sum}}a_{i}\cdot f(a_{i})=a_{1}\cdot f(a_{1})+...+a_{k}\cdot f(a_{k})=\mu$
\end_inset


\end_layout

