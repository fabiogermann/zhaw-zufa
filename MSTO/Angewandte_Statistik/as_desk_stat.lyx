\end_layout

\begin_layout Standard
\begin_inset Formula $\sigma=\sqrt{\frac{1-p}{p^{2}}}$
\end_inset


\end_layout

\begin_layout Section
Hypergeometrische Verteilung
\end_layout

\begin_layout Standard
Verwendung:
\begin_inset Newline newline
\end_inset

Eine Menge bestehe aus N Teilen unter denen sich M Teile mit einem Merkmal
 A befinden.
\begin_inset Newline newline
\end_inset

Man entnimmt n Teile ohne Zurücklegen.
 x zählt die Anzahl der Teile in n mit Merkmal A.
\end_layout

\begin_layout Verse
\begin_inset Formula $q(x)=\frac{\left(\begin{array}{c}
M\\
x
\end{array}\right)\left(\begin{array}{c}
N-M\\
n-x
\end{array}\right)}{\left(\begin{array}{c}
N\\
n
\end{array}\right)}$
\end_inset


\end_layout

\begin_layout Standard
Erwartungswert
\end_layout

\begin_layout Verse
\begin_inset Formula $E(X)=n\frac{M}{N}$
\end_inset


\end_layout

