\begin_layout Subsubsection
Häufigkeiten
\end_layout

\begin_layout Verse
Umfang der Stichprobe: 
\begin_inset Formula $n$
\end_inset


\end_layout

\begin_layout Verse
absolute Häufigkeit: 
\begin_inset Formula $h(a_{j})=$
\end_inset

 Anzahl der Ausprägungen in der Beobachtungsmenge
\end_layout

\begin_layout Verse
relative Häufigkeit: 
\begin_inset Formula $f(a_{j})=\frac{Anzahl\: der\, Ausprägungen\, in\, der\, Beobachtungsmenge}{Grösse\, der\, Beobachtungsmenge\,(Umfang\, der\, Stichprobe)}=\frac{h(a_{j})}{n}$
\end_inset


\end_layout

\begin_layout Verse
absolute Summenhäufigkeit: 
\begin_inset Formula $G(x)=\overset{n}{\underset{i}{\sum}}h(x)$
\end_inset


\end_layout

\begin_layout Verse
Verteilungsfunktion: 
\begin_inset Formula $H(x)=\sum f(x)=\frac{1}{n}\sum h(x)=\frac{G(x)}{n}$
\end_inset


\end_layout

