\begin_layout Section
Statistische Masszahlen
\end_layout

\begin_layout Subsection
Arithmetisches Mittel
\end_layout

\begin_layout Verse
\begin_inset Formula $\bar{x}=\frac{1}{n}\sum x_{i}=\frac{1}{n}(n_{1}a_{1}+...+n_{k}a_{k})$
\end_inset


\end_layout

\begin_layout Standard
Für Häufigkeitsdaten mit Ausprägungen 
\begin_inset Formula $a_{1},...,a_{k}$
\end_inset

 und relativen Häufigkeiten 
\begin_inset Formula $f(a_{1}),...,f(a_{k})$
\end_inset

 gilt:
\end_layout

\begin_layout Verse
\begin_inset Formula $\bar{x}=\overset{k}{\underset{i=1}{\sum}}a_{i}\cdot f(a_{i})=a_{1}\cdot f(a_{1})+...+a_{k}\cdot f(a_{k})$
\end_inset


\end_layout

\begin_layout Standard
Beispiel mit den Würfeln:
\end_layout

\begin_layout Verse
\begin_inset Formula $\bar{x}=\overset{12}{\underset{x=2}{\sum}}xf(x)=7.013$
\end_inset


\end_layout

\begin_layout Subsection
Modalwert (Modus)
\end_layout

\begin_layout Standard
Der Modalwert gibt an, welche Ausprägung am häufigsten vorkommt.
\end_layout

\begin_layout Itemize
\begin_inset Formula $x_{mod}$
\end_inset

 bei diskreten Merkmalen: Ausprägung mit grösster Häufigkeit
\end_layout

\begin_layout Itemize
\begin_inset Formula $x_{mod}$
\end_inset

 bei stetigen Merkmalen: Maximum der Parabel, resp.
 die Mitte der stärkst besetzten Klasse
\end_layout

